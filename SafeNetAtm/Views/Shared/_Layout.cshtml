<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>SafeNet ATM</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery-inputmask/inputmask.min.js"></script>
    <script src="~/lib/jquery-inputmask/jquery.inputmask.min.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-info border-bottom box-shadow mb-3">
            <div class="container" style="align-items:center;">
                <img class="img-fluid" style="width:30%;"
                     src="https://safenetconsulting.com/wp-content/uploads/2020/01/safenet-logo.png" />
                <h3 style="width:60%;">ATM Machine</h3>
            </div>
        </nav>
    </header>
    <div class="row" style="top: 0;z-index: 9999;padding-bottom:10px;"
         data-spy="affix" data-offset-top="1">
        <div class="col-1"></div>
        <div id="divGeneralAffixMSG" class="col-10 w-100"></div>
        <div class="col-1"></div>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - SafeNet ATM - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
